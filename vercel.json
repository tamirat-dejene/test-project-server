{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",   
      "destination": "https://test-project-l6lsajd3g-tamirat-dejenes-projects.vercel.app/$1",
      "permanent": true    
    }
  ]
}
